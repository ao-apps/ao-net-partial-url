<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- Consistency with standard "URLDecoder", "URLEncoder", and "URL" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]MultiPartialURL\.java$"
    message="'MultiPartialURL'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]MultiPartialURLTest\.java$"
    message="'(MultiPartialURLTest|test.*URL.*)'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]PartialURL\.java$"
    message="'(PartialURL|toURL)'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]PartialURLMap\.java$"
    message="'PartialURLMap'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]PartialURLMapTest\.java$"
    message="'PartialURLMapTest'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]PartialURLMatch\.java$"
    message="'(PartialURLMatch|getPartialURL|getSingleURL)'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]PartialURLTest\.java$"
    message="'PartialURLTest'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]SinglePartialURL\.java$"
    message="'SinglePartialURL'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]SinglePartialURLTest\.java$"
    message="'(SinglePartialURLTest|test.*URL.*)'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]net[/\\]partialurl[/\\]URLFieldSource\.java$"
    message="'URLFieldSource'"
  />

</suppressions>
